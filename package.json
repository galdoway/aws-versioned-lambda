{
  "name": "aws-versioned-lambda",
  "type": "module",
  "scripts": {
    "build": "esbuild --bundle --outfile=./dist/index.js --platform=node --target=node22 ./src/index.ts",
    "zip": "zip -j lambda.zip dist/index.js",
    "update": "aws lambda update-function-code --zip-file fileb://lambda.zip --function-name aws-versioned-lambda --profile root",
    "deploy": "run-s build zip update",
    "clean": "rm lambda.zip"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "esbuild": "^0.21.4",
    "npm-run-all2": "^6.2.0"
  },
  "packageManager": "pnpm@10.15.0"
}
